{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "",
  "main": "server/app.js",
  "repository": "https://github.com/Dikaeinstein/book-a-meal.git",
  "author": "Solomon Okwa <solozyokwa@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=production || set NODE_ENV=production&& yarn db-migrate && node server/dist/app.js",
    "build": "babel server -d server/dist",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate-dev": "sequelize db:migrate --env development",
    "db-migrate-test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "start-dev": "export NODE_ENV=development || set NODE_ENV=development&& yarn db-migrate-dev && nodemon --exec babel-node server/app.js",
    "heroku-postbuild": "yarn build",
    "test-client:watch": "jest --watchAll",
    "test-client": "jest",
    "test-single": "export NODE_ENV=test || set NODE_ENV=test&& yarn db-migrate-test && nyc --require babel-core/register mocha server/test/**.spec.js -t 5000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "gh-pages -d UI",
    "sequelize": "sequelize",
    "start-client:dev": "export NODE_ENV=development || set NODE_ENV=development&& webpack-dev-server --mode development --config webpack.dev.js --open",
    "start-client": "webpack --mode production --config webpack.prod.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "formik": "^0.11.11",
    "gh-pages": "^1.1.0",
    "history": "^4.7.2",
    "immer": "^1.3.1",
    "jsonwebtoken": "^8.2.1",
    "lodash.isempty": "^4.4.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-materialize": "^2.2.1",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-spinners": "^0.3.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "swagger-ui-express": "^3.0.8",
    "validator": "^9.4.1",
    "yamljs": "^0.3.0",
    "yup": "^0.25.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.0",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "ghooks": "^2.0.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jsdom": "^11.10.0",
    "mini-css-extract-plugin": "^0.2.0",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "prop-types": "^15.6.1",
    "react-hot-loader": "^4.2.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "nyc": {
    "exclude": [
      "**/index.js",
      "**/app.js",
      "**/config.js"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn test-single"
    }
  },
  "engines": {
    "yarn": "1.7.0",
    "node": "8.11.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/*",
      "client/*"
    ]
  }
}
